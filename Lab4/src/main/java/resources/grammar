terminals:
  - xor
  - or
  - and
  - not
  - ( : WHITESPACELESS
  - ) : WHITESPACELESS
  - var : VARIABLE
non-terminals:
  - S
  - XOR
  - OR
  - AND
  - NOT
  - BRACKETS
start-non-terminal: S
rules:
  - S -> XOR[([1].value, [0].a, [0].b) -> args[0] = (int)args[1] + (int)args[2];]
  - XOR -> OR xor XOR
  - XOR -> OR
  - OR -> AND or OR
  - OR -> AND
  - AND -> NOT and AND
  - AND -> NOT
  - NOT -> not NOT
  - NOT -> BRACKETS
  - BRACKETS -> var